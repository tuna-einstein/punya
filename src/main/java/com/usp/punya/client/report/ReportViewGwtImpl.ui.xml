<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:p='urn:import:com.vaadin.polymer.paper.widget'
	xmlns:i='urn:import:com.vaadin.polymer.iron.widget'>

	<g:HTMLPanel>
		<paper-icon-item ui:field="menuClearAll">
			<iron-icon item-icon="delete"/>
			<div>Clear All</div>
		</paper-icon-item>
		<paper-icon-item ui:field="menuClearDone">
			<iron-icon item-icon="clear" />
			<div>Clear Done</div>
		</paper-icon-item>
		<paper-icon-item ui:field="menuSettings">
			<iron-icon icon="settings"  />
			<div>Settings</div>
		</paper-icon-item>
		<paper-icon-item ui:field="menuAbout">
			<iron-icon icon="help"  />
			<div>About</div>
		</paper-icon-item>
	</g:HTMLPanel>
</ui:UiBinder>